{
    "parser": "@typescript-eslint/parser",
    "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
],
    "parserOptions": {
    "ecmaVersion": 2020,
        "sourceType": "module"
},
    "env": {
    "browser": true,
        "es6": true,
        "node": true
},
    "rules": {
    "no-console": "warn",
        "no-debugger": "warn",
        "eqeqeq": ["error", "always"],
        "curly": "error",
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "max-len": [
        "error",
        {
            "code": 80,
            "ignoreUrls": true
        }
    ],
        "@typescript-eslint/no-explicit-any": "warn",
        "@typescript-eslint/explicit-function-return-type": "off",
        "@typescript-eslint/no-unused-vars": [
        "error",
        {
            "argsIgnorePattern": "^_"
        }
    ]
},
    "settings": {
    "react": {
        "version": "detect"
    }
}
}
